{
  "name": "teamplate",
  "version": "0.1.0",
  "private": true,
  "main": "resources/server.js",
  "scripts": {
    "watch:typescript": "babel src/react --out-dir src/ecmascript --watch --extensions .ts,.tsx,.jsx",
    "watch:css": "postcss src/css/app.css -w --dir public/app --verbose",
    "watch:clojurescript": "shadow-cljs watch app server",
    "watch:server": "node --watch resources/server.js",
    "release:typescript": "babel src/react --out-dir src/ecmascript --extensions .ts,.tsx,.jsx",
    "release:css": "postcss src/css/app.css --dir public/app",
    "release:clojurescript": "shadow-cljs release app server",
    "start": "conc -c auto npm:watch:*",
    "release": "conc -c auto npm:release:* && npm run bundle:optimize && node resources/server.js",
    "bundle": "esbuild --bundle public/app/lib.js --outfile=public/app/lib.bundle.js",
    "watch:bundle": "npm run bundle -- --watch",
    "bundle:optimize": "npm run bundle --  --analyze --metafile=.bundle.meta.json --define:NODE_ENV=production --minify"
  },
  "dependencies": {
    "@guildxyz/sdk": "^2.5.0",
    "@guildxyz/types": "^1.9.10",
    "@mantine/core": "^7.4.2",
    "@mantine/hooks": "^7.4.2",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.106.0",
    "@react-three/fiber": "^8.16.8",
    "@react-three/rapier": "^1.3.1",
    "@tabler/icons-react": "^3.5.0",
    "@tanstack/react-query": "^5.40.1",
    "@types/three": "^0.165.0",
    "@use-gesture/core": "^10.3.1",
    "buffer": "^6.0.3",
    "ecctrl": "^1.0.79",
    "esbuild": "^0.20.1",
    "express": "^4.19.2",
    "howler": "^2.2.4",
    "http": "^0.0.1-security",
    "leva": "^0.9.35",
    "ollama": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-refresh": "^0.13.0",
    "three": "^0.165.0",
    "viem": "^2.13.7",
    "vite": "^5.3.1",
    "wagmi": "^2.10.2",
    "ws": "^8.17.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@testing-library/react": "^14.1.2",
    "babel": "^6.23.0",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "global-jsdom": "9.0.1",
    "jsdom": "22.0.0",
    "jsdom-global": "3.0.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-preset-mantine": "^1.12.3",
    "postcss-simple-vars": "^7.0.1",
    "shadow-cljs": "^2.27.1"
  }
}
